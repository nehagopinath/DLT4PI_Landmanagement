{
    "name": "first-network-org4",
    "version": "1.0.0",
    "client": {
        "organization": "Org4",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "channels": {
        "fit4ghana": {
            "orderers":  [
                "orderer.example.com"
            ],
            "peers": [
                "peer0.org4.example.com",
                "peer1.org4.example.com"
            ]
        }
    },
    "organizations": {
        "Org4": {
            "mspid": "Org4MSP",
            "peers": [
                "peer0.org4.example.com",
                "peer1.org4.example.com"
            ],
            "certificateAuthorities": [
                "ca_peerOrg4"
            ]
        }
    },
    "peers": {
        "peer0.org4.example.com": {
            "url": "grpcs://localhost:11051",
            "tlsCACerts": {
                "path": "../../first-network/crypto-config/peerOrganizations/org4.example.com/tlsca/tlsca.org4.example.com-cert.pem"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.org4.example.com"
            }
        },
        "peer1.org4.example.com": {
            "url": "grpcs://localhost:12051",
            "tlsCACerts": {
                "path": "../../first-network/crypto-config/peerOrganizations/org4.example.com/tlsca/tlsca.org4.example.com-cert.pem"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer1.org4.example.com"
            }
        }
    },
    "certificateAuthorities": {
        "ca_peerOrg4": {
            "url": "https://localhost:7054",
            "caName": "ca-org4",
            "tlsCACerts": {
                "path": "../../first-network/crypto-config/peerOrganizations/org4.example.com/tlsca/tlsca.org4.example.com-cert.pem"
            },
            "httpOptions": {
                "verify": false
            },
            "registrar": [
                {
                    "enrollId": "admin",
                    "enrollSecret": "adminpw"
                }
            ]
        }
    },
    "discovery" : {
        "enabled": true,
        "asLocalhost": false }
}
